{
  "name": "ng-ngrid2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "testlocal": "ng build ngrid2 && ng serve -o",
    "clean": "del .\\ngrid2-*.tgz",
    "upversion": "cd .\\projects\\ngrid2 && npm version patch",
    "pack": "ng build ngrid2 && copy .\\LICENSE .\\dist\\ngrid2 && copy .\\README.md .\\dist\\ngrid2 && npm pack .\\dist\\ngrid2",
    "publish": "npm login && for %X in (ngrid2-*.tgz) do npm publish .\\%X",    
    "gitpages": "ng build --prod --output-path docs --base-href ngNgrid2 && copy .\\docs\\index.html .\\docs\\404.html",    
    "packpublish": "npm run clean && npm run upversion && npm run pack && npm run publish && npm run gitpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "core-js": "^2.5.4",
    "npm": "^6.4.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.11",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.5",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
